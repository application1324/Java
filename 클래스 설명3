package chap_07;

public class _04_Method {
	
	public static void main(String[] args) {
		BlackBox b1 = new BlackBox();
		b1.modeIName = "까망이";
		
		System.out.println(b1.canAutoReport);
		
		b1.canAutoReport = true;
		b1.autoReprt();
		
		b1.insertMenoryCard(512);
//		자동 충돌 감지 업데이트 완료
		b1.canAutoReport = true;
		
	}
}
