package chap_04;

public class _09_MultipleTable {
	public static void main(String[] args) {
	
		for(int i=2; i<10; i++) { //시작한수-지정한수만큼 실행
			
			for(int j=1; j<10; j++) {
				
				System.out.println(i+"*"+j+"="+(i*j));
//				()하는것은 가독성을 높이기 위해 사용
//				사용하지 않아도 상관은 없음
//				왠만하면 사용하는것이 좋아보임
				
			}
			System.out.println();
		}
	
	}
		
}
