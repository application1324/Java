package test2;

import java.util.Scanner;

public class Ex09 {

//	직사각형 넓이 구하기(매소드 이용)
//	main에서 area 매소트 호출
//	area 메소드는 가로와 세로의 길이를 파라미터롤 받아서
//	넓이값을 리턴해주는 매소드 이다.
	
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();
		int num1 = sc.nextInt();
		
		System.out.println(area(num, num1));
		
		sc.close();
	}
	
	public static int area(int wi, int hi) {
		return wi * hi;
	}
	
	
}
